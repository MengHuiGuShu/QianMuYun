<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!--*********************************自己定义的字体类型@杨杰豪********************************************-->
    <style name="MyTitle" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">24sp</item>
    </style>
    <!--*********************************自己定义的字体类型@杨杰豪********************************************-->
    <style name="MyNormal" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!--*********************************自己定义的字体类型@杨杰豪********************************************-->
    <style name="MyTime" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
    </style>
    <!--*********************************自己定义的字体类型@杨杰豪********************************************-->
    <style name="MyNormalNoBackgroud" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!--*********************************自己定义的字体类型@杨杰豪********************************************-->
    <style name="MySearch" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:imeOptions">actionSearch</item>
    </style>
    <!--*********************************自己定义的页面背景色@杨杰豪********************************************-->
    <style name="PageAllBackgroud">
        <item name="android:background">@color/backgroumdLab</item>
    </style>
    <!--*********************************自己定义的页面背景色@刘凯********************************************-->
    <style name="styleview">
        <item name="android:layout_width">0dp</item>
        <item name="height">0.5dp</item>
        <item name="android:background">@color/TitleUser</item>
    </style>
    <!--*********************************自己定义的页面背景色@刘凯********************************************-->
    <style name="textkaikai">
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <!--*********************************自己定义的页面背景色@刘凯********************************************-->
    <style name="radiosize">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/TitleUser</item>
    </style>
    <!--*********************************自己定义的页面背景色@刘凯********************************************-->
    <style name="input">
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>
    <!--*********************************自己定义的页面背景色@刘凯********************************************-->
    <style name="inputsize">
        <item name="android:textSize">14sp</item>
        <item name="android:background">@color/backgroumdLab</item>
        <item name="android:hint">  </item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/TitleUser</item>
    </style>
    <!--*********************************自己定义的页面背景色@杨杰豪********************************************-->
    <style name="TableEditFragment" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:layout_height">32dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/TitleUser</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:imeOptions">actionDone</item>
    </style>


</resources>
